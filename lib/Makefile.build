# -*- Makefile -*-

src    :=
deps   := ../bin/BouncyCastle.dll ../bin/CoreCrypto.dll
target  = ../bin/TLSLib.dll

include ../Makefile.config

.PHONY: all dist

all: $(target)

dist:
	cp Makefile.config $(src) $(distdir)

$(target): $(src) $(deps)
	$(fsc) $(fsflags) --warn:0 $(addprefix -r ,$(deps)) -a -o $@ $(src)

Makefile.config: lib.fsproj
	-../makegen -o $@ $<

include Makefile.config
